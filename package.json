{
  "name": "vr",
  "version": "0.1.0",
  "description": "Just some virtual reality experiments.",
  "engines": {
    "node": "^6.11.0",
    "npm": "^5.3.0"
  },
  "scripts": {
    "build": "browserify src/index.js -o bundle.js",
    "lint": "eslint --fix .",
    "precommit": "npm run lint",
    "start": "concurrently -c green,yellow \"watchify src/index.js -o bundle.js -v\" \"live-server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oparamo/vr.git"
  },
  "keywords": [
    "aframe",
    "a-frame",
    "vr",
    "virtual reality",
    "webvr"
  ],
  "author": "Oscar Paramo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oparamo/vr/issues"
  },
  "homepage": "https://github.com/oparamo/vr#readme",
  "devDependencies": {
    "browserify": "14.4.0",
    "concurrently": "3.5.0",
    "eslint": "4.3.0",
    "husky": "0.14.3",
    "live-server": "1.2.0",
    "watchify": "3.9.0"
  },
  "dependencies": {
    "aframe": "0.6.1"
  },
  "eslintIgnore": [
    "bundle.js"
  ]
}
